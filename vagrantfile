
$script = <<SCRIPT
gvm install go2
SCRIPT
Vagrant.configure("2") do |config|
  vmName = "golangUT"
  config.vm.box = "bento/ubuntu-16.04"
  config.vm.provider "virtualbox" do |v|
      v.customize ["modifyvm", :id, "--cableconnected1", "on"]
      end
  config.vm.provision "shell", inline: $script, privileged: false
end
